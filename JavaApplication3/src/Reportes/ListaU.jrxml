	<queryString>
		<![CDATA[(SELECT dc.codigo as codigo,
			concat(u.Nombre,' ',u.APaterno,' ',u.AMaterno)as nombre,
			COUNT(m.Usuario_IdUsuario) as Total_Movimientos
    from Usuario u,Mov_Auditoria m, Docente dc
    where u.IdUsuario=m.Usuario_IdUsuario and dc.Usuario_IdUsuario=u.IdUsuario and u.IdUsuario in
	(Select Usuario_IdUsuario from Docente where Usuario_IdUsuario=u.IdUsuario)having COUNT(m.Usuario_IdUsuario)>0) 
    union
    (SELECT a.codigo as codigo,
			concat(u.Nombre,' ',u.APaterno,' ',u.AMaterno)as nombre,
			COUNT(m.Usuario_IdUsuario) as Total_Movimientos
    from Usuario u,Mov_Auditoria m, Alumno a
    where u.IdUsuario=m.Usuario_IdUsuario and a.Usuario_IdUsuario=u.IdUsuario and u.IdUsuario in
	(Select Usuario_IdUsuario from Alumno where Usuario_IdUsuario=u.IdUsuario)having COUNT(m.Usuario_IdUsuario)>0)
    order by Total_Movimientos ;]]>
	</queryString>
