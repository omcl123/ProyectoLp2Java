	<queryString>
		<![CDATA[(SELECT dc.Codigo as codigo,concat(u.Nombre,' ',u.APaterno,' ',u.AMaterno)as nombre,a.Nombre as movimiento,d.Nombre as documento,m.Fecha as fecha
	from Mov_Auditoria m,Usuario u, Documentos d, Accion a,Docente dc
	where m.Usuario_IdUsuario=u.IdUsuario and m.Documentos_Id=d.Id and u.IdUsuario=dc.Usuario_IdUsuario
	and m.Accion_idAccion=a.idAccion and u.IdUsuario in
	(Select Usuario_IdUsuario from Docente where Usuario_IdUsuario=u.IdUsuario))
	union
	(SELECT am.Codigo as codigo,concat(u.Nombre,' ',u.APaterno,' ',u.AMaterno)as nombre,a.Nombre as movimiento,d.Nombre as documento,m.Fecha as fecha
	from Mov_Auditoria m,Usuario u, Documentos d, Accion a,Alumno am
	where m.Usuario_IdUsuario=u.IdUsuario and m.Documentos_Id=d.Id and am.Usuario_IdUsuario=u.IdUsuario
	and m.Accion_idAccion=a.idAccion and u.IdUsuario in
	(Select Usuario_IdUsuario from Alumno where Usuario_IdUsuario=u.IdUsuario))
	order by fecha desc ;]]>
	</queryString>
